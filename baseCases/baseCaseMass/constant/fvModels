reactionSourceCOMassIW
{
    type            coded;
    active          yes;
    name	        reactionSourceCOMassIW;
    selectionMode   all;

    field 		        COMass;

    codeInclude
    #{
        #include "fvCFD.H"
    #};

    codeAddRhoSup
    #{        
        #include "../../constant/rnTerms/loadCommonData.H"      
        
        word currSpName("CO");
        
        #include "../../constant/rnTerms/r_powerLawV3.H"                 
    #};
}

