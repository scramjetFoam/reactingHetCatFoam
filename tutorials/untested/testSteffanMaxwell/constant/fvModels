reactionSourceAMassIW
{
    type            coded;
    active          yes;
    name	        reactionSourceCOIW;
    selectionMode   all;

    // field 		        CO;
    field 		        AMass;

    codeInclude
    #{
        #include "fvCFD.H"
    #};

    codeAddRhoSup
    #{        
        #include "../../constant/rnTerms/loadCommonData.H"      
        
        word currSpName("A");
        
        #include "../../constant/rnTerms/r_powerLawV3.H"                 
    #};
}

