regions
(
    // Set patch values (using ==)
    boxToCell
    {
        box (0.09595865269206261 -1 -10) (1 1 10);

        fieldValues
        (
            volScalarFieldValue H2Mass 0.06821646
            volScalarFieldValue CO2Mass 0
            volScalarFieldValue N2Mass 0.93178354
        );
    }
);