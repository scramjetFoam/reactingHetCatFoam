        value           uniform 501325;
    
        // Optional entries
        U               U;
        phi             phi;
    }
    "(walls|inletCyl)"
    {
        type            zeroGradient;
    }
    
    cylinder
//   sides 
    {
	    type zeroGradient;
        // type            codedMixed;
