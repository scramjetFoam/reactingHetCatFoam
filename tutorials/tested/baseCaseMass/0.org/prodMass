FoamFile
{
    version     1.0;
    format      ascii;
    class       volScalarField;
    object      CO;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    sides
    {
        type            zeroGradient;
    }
    sphere
    {
        type            fixedValue;
        value           uniform 0;
    }
        sphere_slave
    {
        type            fixedValue;
        value           uniform 0;
    }
    
}

