    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvModels;
}

reactionSourceEthyleneMassIW
{
    type            coded;
    active          yes;
    name	        reactionSourceEthyleneMassIW;
    selectionMode   all;

    field 		        ethyleneMass;

    codeInclude
    #{
        #include "fvCFD.H"
    #};

    codeAddRhoSup
    #{        
        #include "../../constant/rnTerms/loadCommonData.H"      
        
        word currSpName("ethylene");
        
        #include "../../constant/rnTerms/r_powerLawV3.H"                 
    #};
}

