FoamFile
{
    version     1.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel  Newtonian;

// -- list of chemical species that are assumed
species (ethylene O2 prod N2);

// -- list of porous Zones
zones (coatZone);


// ************************************************************************* //
// -- information about individual chemical species
// -- ref == reference (carrier) gas -- used for caluculation of the enthalpy transport, and Fick diffusion coefficient, if not set diffusion coefficient directly
ref
{
    // -- reference diffusion volume in m3mol-1 for diffusivity computations (see Fuller)
    sigmaV      16.2;                                                   

    // -- thermal conductivity polynom coeficients for reference gas Wm-1K-1
    // -- kappaGas = kappaA+kappaB*T+kappaC*Foam::pow(T,2)+kappaD*Foam::pow(T,3)
    kappaA  kappaA  [1 1 -3 -1 0 0 0] 0.3918e-3;                             
    kappaB  kappaB  [1 1 -3 -2 0 0 0] 0.9814e-4;                              
    kappaC  kappaC  [1 1 -3 -3 0 0 0] -5.0660e-8;                             
    kappaD  kappaD  [1 1 -3 -4 0 0 0] 1.503479e-11;

    //specific heat of the substrate in Jkg-1K-1
    // Cp	    Cp 	    [0  2  -2   -1  0 0 0]  520.65;                       
}

ethylene
{
    
    sigmaV      18.0;   // -- diffusion volume in Fuller                                                   
    molM        28.01e-3;   // -- molar mass in kgmol-1
    // D  D        [0 2 -1 0 0 0 0] DSet;  // -- diffusion coef. (option to set it directly)
    // -- stoichiometric coefficients in reactions
    nuVec       (0.0 0.0 -1.0);   //row of the stoechiometric matrix                                                   
    // alphaVec       (0.0 0.0 1.0);   //row of the exponents alpha in kinetics                           
}

// -- for the description of the dictionary, see above
O2
{
    sigmaV      18.0;
    molM        28.01e-3;
    nuVec       (0.0 0.0 -0.5);                              
    // alphaVec    (0.0 0.0);                                     
}

HCl
{
    sigmaV      18.0;
    molM        28.01e-3;
    nuVec       (0.0 0.0 -2.0);                              
    // alphaVec    (0.0 0.0 );                                     
}


C2H4Cl2
{
    sigmaV      18.0;
    molM        28.01e-3;
    nuVec       (0.0 0.0 1.0);                              
    // alphaVec    (0.0 0.0);                                     
}

H2O
{
    sigmaV      18.0;
    molM        28.01e-3;
    nuVec       (0.0 0.0 1.0);                              
    // alphaVec    (0.0 0.0);                                     
}

N2
{
    sigmaV      18.0;
    molM        28.01e-3;
    nuVec       (0.0 0.0);                              
    // alphaVec       (0.0 0.0);                                
}

// ************************************************************************* //
// -- information about the porous zones
coatZone    
{   
    porEps      porEps      [0 0 0 0 0 0 0]         0.5;    //material porosity
    tort        tort        [0 0 0 0 0 0 0]         tortSet;    //material tortuosity
    dP	        dP	        [0 1 0 0 0 0 0]         1e-5;    //Mean pore size (for Knudsen diffusion (effectively turn off here))
    kappa       kappa       [1 1 -3 -1 0 0 0]       kappaEffSet;    //thermal conductivity of material in Wm-1K-1
    Cp          Cp          [0 2 -2 -1 0 0 0]       962;    //specific heat of the substrate in Jkg-1K-1
    rho         rho         [1 -3 0 0 0 0 0]        1000.0; //efective density in kgm-3 (assumed constant)
    coatFrac    coatFrac    [0 0 0 0 0 0 0]         1.0;    //coating fraction in zone
}

// ************************************************************************* //
// -- general constants
genProps
{
    univR   univR   [1  2 -2 -1 -1 0 0]  8.31446261815324;
}


