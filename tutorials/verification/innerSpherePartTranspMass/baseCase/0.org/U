FoamFile
{
    version     1.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField	uniform	(0 0 0.0);

boundaryField
{
    infinity
    {
        type            fixedValue;
	    value	uniform (0 0 0.0);
    }
    sphere
    {
        type            fixedValue;
	    value	uniform (0 0 0.0);
    }
}


