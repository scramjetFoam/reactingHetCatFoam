FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      residuals;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

residuals
    {
        functionObjectLibs ("libutilityFunctionObjects.so");
        type    residuals;
        writeControl    timeStep;
        writeInterval   1;
        fields
        (
            U
            p
	        T
	        CO
            O2
            CO2
        );
    }

