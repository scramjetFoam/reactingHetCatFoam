    // molM        32.0e-3;                                                //molar mass in kgmol-1
    molM        28.96e-3;                                                //molar mass in kgmol-1
    sigmaVR     16.2;                                                   //diffusion volume of the reference gas in m3mol-1
    molMR       28.0e-3;                                                //molar mass of the reference gas kgmol-1
    
