    B           7.344e-2;                                               // A + B*T + C *T**2 + D*T**3 //molar heat capacity 
    C           -0.5602e-4;                                             // A + B*T + C *T**2 + D*T**3 //molar heat capacity 
    D           1.715e-8;                                               // A + B*T + C *T**2 + D*T**3 //molar heat capacity 
    
    
