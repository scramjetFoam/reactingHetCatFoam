FoamFile 
{ 
 	 version 	 1.0; 
 	 format 	 ascii; 
 	 class 		 dictionary; 
 	 location 	 "constant"; 
 	 object 	 porosityProperties; 
} 
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * // 

reactingCellZone
{
	type		DarcyForchheimer;
	active		yes;
	cellZone	reactingCellZone;

	DarcyForchheimerCoeffs
	{
		d	(1.0e+12 1.0e+12 1.0e+12);
		f	(0.000000e+00 0.000000e+00 0.000000e+00);

		coordinateSystem
		{
			type		cartesian;
			origin		(0 0 0);
			coordinateRotation
			{
				type	axesRotation;
				e1		(1 0 0);
				e2		(0 0 1);
			}
		}
	}
}

