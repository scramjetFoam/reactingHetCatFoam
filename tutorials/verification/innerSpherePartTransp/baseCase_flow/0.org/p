    {
        // type    totalPressure;
        // p0      uniform 101325;
        // value   uniform 101325;
    
        // U       U;
        // phi     phi;
        type            zeroGradient;
    }
    outlet
    {
        // type    zeroGradient;
        type            totalPressure;
        p0              uniform 101325;
        value           uniform 101325;
    
        U               U;
        phi             phi;
    }
    sides
    {
        type    symmetry;
    }
